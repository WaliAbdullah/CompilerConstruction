$ A program to compute the factorial number. Input n; output n!  

begin 
	integer f; 
	proc fact 
	 begin 
	       integer x, y; 
	       if f > 1 ->  
		       x := f; 
		       f := x - 1; 
		       call fact; 
		       y := f; 
		       f := x*y; 
	       []  ~(f > 1) -> skip; 
	       fi; 
	 end; 
read f; 
call fact; 
write f; 
end.
